{
  "name": "keep-a-changelog-new-release-action",
  "version": "1.0.0",
  "private": true,
  "description": "A GitHub action for updating your Keep-A-Changelog CHANGELOG.md for new releases",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "git config core.hooksPath .githooks",
    "build": "webpack --mode production",
    "compile": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomaseizinger/keep-a-changelog-new-release.git"
  },
  "author": "Thomas Eizinger",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@actions/exec": "^1.0.3",
    "remark-parse": "^10.0.1",
    "remark-stringify": "^10.0.2",
    "stream-buffers": "^3.0.2",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/stream-buffers": "^3.0.3",
    "@types/unist": "^2.0.3",
    "@types/vfile": "^4.0.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-plugin-github": "^4.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.4.2",
    "jest-circus": "^29.4.2",
    "js-yaml": "^4.1.0",
    "mock-env": "^0.2.0",
    "prettier": "^2.8.4",
    "to-vfile": "^7.2.4",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
